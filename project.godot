; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SkateLab Mod Manager"
config/description="A Mod Manager For The Game Skatelab"
config/version="1.0.1"
run/main_scene="res://Other/Main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="SkateLab-Mod-Manager"
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=24
run/low_processor_mode=true
run/delta_smoothing=false
boot_splash/image="res://Assets/Splash.png"
boot_splash/use_filter=false
config/icon="res://Assets/Icon.png"
boot_splash/minimum_display_time=3250

[autoload]

autoload="*res://Scripts/autoload.gd"

[display]

window/size/viewport_width=1024
window/size/viewport_height=768
window/size/resizable=false
window/vsync/vsync_mode=0

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
